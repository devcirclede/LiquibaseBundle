{
    "name": "freesoftde/liquibase-bundle",
    "description": "Liquibase Bundle for Symfony 5",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Freesoftde\\LiquibaseBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Freesoftde\\LiquibaseBundle\\Test\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "vansari",
            "email": "junker.kurt@googlemail.com"
        }
    ],
    "require": {
        "php": ">=7.3 <=9.0",
        "symfony/framework-bundle": "^5.3",
        "symfony/console": "^5.3",
        "ext-curl": "*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "squizlabs/php_codesniffer": "^3.6",
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "unit": "vendor/bin/phpunit -c phpunit.dist.xml --debug -vv"
    }
}
